{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "experimentalObjectRestSpread": true
    },
    "rules": {
        "no-console": 0,
        "semi": 0,
        "prefer-const": "error"
    }
}